{
  "name": "aws-github-actions",
  "version": "0.1.0",
  "description": "GitHub actions for AWS services",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clowdhaus/aws-github-actions.git"
  },
  "keywords": [
    "github",
    "actions",
    "aws",
    "cloudfront",
    "s3"
  ],
  "author": "Clowd Haus, LLC",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/clowdhaus/aws-github-actions/issues"
  },
  "homepage": "https://github.com/clowdhaus/aws-github-actions#readme",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "lerna run test",
    "compile": "lerna run build && lerna run compile",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts}' --quiet --fix",
    "release": "lerna version --conventional-commits --yes",
    "diff": "lerna diff",
    "commit": "npx git-cz"
  },
  "dependencies": {
    "@actions/core": "^1.2.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@zeit/ncc": "^0.22.1",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lerna": "^3.20.2",
    "lint-staged": "^10.1.2",
    "prettier": "^2.0.4",
    "typescript": "^3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  }
}
