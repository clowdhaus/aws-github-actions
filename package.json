{
  "name": "aws-github-actions",
  "version": "0.0.0",
  "description": "GitHub actions for AWS services",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clowdhaus/aws-github-actions.git"
  },
  "keywords": [
    "github",
    "actions",
    "aws",
    "cloudfront",
    "s3"
  ],
  "author": "GitHub",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/clowdhaus/aws-github-actions/issues"
  },
  "homepage": "https://github.com/clowdhaus/aws-github-actions#readme",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "lerna run test",
    "compile": "lerna run build && lerna run compile",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts}' --quiet --fix",
    "new-version": "lerna version --conventional-commits --yes",
    "diff": "lerna diff",
    "commit": "npx git-cz"
  },
  "dependencies": {
    "@actions/core": "^1.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "@zeit/ncc": "^0.21.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.1.0",
    "lerna": "^3.20.2",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  }
}
